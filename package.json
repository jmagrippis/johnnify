{
  "name": "johnnify",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:e2e": "playwright test",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "generate:supabase": "supabase gen types typescript --local > src/lib/generated/DatabaseDefinitions.ts",
    "generate:json2ts": "json2ts -i 'json-schemas/**/*.json' -o src/lib/generated --format=false",
    "generate": "bun run generate:supabase && bun run generate:json2ts && prettier --write ./src/lib/generated"
  },
  "devDependencies": {
    "@fontsource-variable/inconsolata": "^5.0.15",
    "@fontsource/noto-color-emoji": "^5.0.16",
    "@playwright/test": "^1.39.0",
    "@poppanator/sveltekit-svg": "^4.1.3",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.38.4",
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@sveltejs/kit": "^1.27.4",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "bun-types": "^1.0.11",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.0",
    "gray-matter": "^4.0.3",
    "json-schema-to-typescript": "^13.1.1",
    "lucide-svelte": "^0.292.0",
    "marked": "^10.0.0",
    "marked-smartypants": "^1.1.4",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "shiki": "^0.14.5",
    "stripe": "^14.4.0",
    "supabase": "^1.110.1",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "svgo": "^3.0.3",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "trustedDependencies": [
    "supabase"
  ],
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "volta": {
    "node": "18.18.2"
  }
}
