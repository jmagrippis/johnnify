{
	"name": "johnnify",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:e2e": "playwright test",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"generate:supabase": "supabase gen types typescript --local > src/lib/generated/DatabaseDefinitions.ts",
		"generate:json2ts": "json2ts -i 'json-schemas/**/*.json' -o src/lib/generated --format=false",
		"generate": "bun run generate:supabase && bun run generate:json2ts && prettier --write ./src/lib/generated"
	},
	"devDependencies": {
		"@fontsource-variable/inconsolata": "^5.0.15",
		"@fontsource/noto-color-emoji": "^5.0.16",
		"@playwright/test": "^1.39.0",
		"@poppanator/sveltekit-svg": "^4.1.3",
		"@supabase/auth-helpers-sveltekit": "^0.10.7",
		"@supabase/supabase-js": "^2.38.4",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.27.3",
		"@tailwindcss/forms": "^0.5.6",
		"@tailwindcss/typography": "^0.5.10",
		"@typescript-eslint/eslint-plugin": "^6.10.0",
		"@typescript-eslint/parser": "^6.10.0",
		"autoprefixer": "^10.4.16",
		"bun-types": "^1.0.10",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.0",
		"gray-matter": "^4.0.3",
		"highlight.js": "^11.9.0",
		"json-schema-to-typescript": "^13.1.1",
		"marked": "^9.1.5",
		"marked-highlight": "^2.0.6",
		"marked-smartypants": "^1.1.3",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.6",
		"stripe": "^14.3.0",
		"supabase": "^1.110.1",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"svgo": "^3.0.2",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0"
	},
	"trustedDependencies": [
		"@sveltejs/kit",
		"supabase"
	],
	"type": "module",
	"engines": {
		"node": "18.x"
	},
	"volta": {
		"node": "18.18.2"
	}
}
