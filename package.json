{
	"name": "johnnify",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:e2e": "playwright test",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"generate:supabase": "supabase gen types typescript --local > src/lib/generated/DatabaseDefinitions.ts",
		"generate:json2ts": "json2ts -i 'json-schemas/**/*.json' -o src/lib/generated --format=false",
		"generate": "bun run generate:supabase && bun run generate:json2ts && prettier --write ./src/lib/generated",
		"update": "ncu -ui --format group --install always"
	},
	"devDependencies": {
		"@fontsource-variable/inconsolata": "^5.0.16",
		"@playwright/test": "^1.41.2",
		"@poppanator/sveltekit-svg": "^4.2.1",
		"@shikijs/rehype": "^1.1.3",
		"@supabase/ssr": "^0.1.0",
		"@supabase/supabase-js": "^2.39.6",
		"@sveltejs/adapter-vercel": "^5.1.0",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
		"@typescript-eslint/parser": "^7.0.1",
		"autoprefixer": "^10.4.17",
		"bun-types": "^1.0.27",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"gpt-tokenizer": "^2.1.2",
		"gray-matter": "^4.0.3",
		"json-schema-to-typescript": "^13.1.2",
		"lucide-svelte": "^0.331.0",
		"npm-check-updates": "^16.14.15",
		"openai": "^4.28.0",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.1",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"rehype-stringify": "^10.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.0",
		"shiki": "^1.1.3",
		"stripe": "^14.17.0",
		"supabase": "^1.142.2",
		"svelte": "^4.2.11",
		"svelte-check": "^3.6.4",
		"svgo": "^3.2.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"unified": "^11.0.4",
		"vite": "^5.1.3"
	},
	"trustedDependencies": [
		"npm-check-updates",
		"supabase"
	],
	"type": "module",
	"engines": {
		"node": "20.x"
	},
	"volta": {
		"node": "20.11.1"
	}
}
